    "dotenv": "^16.6.1",
    "zod": "^3.25.76",
    "commander": "^11.1.0",
    "chalk": "^5.6.2",
    "inquirer": "^9.3.8",
    "node-cron": "^3.0.3",
    "twitter-api-v2": "^1.27.0",
    "axios": "^1.12.2",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/bun": "latest",
