{
  "name": "social-media-orchestrator",
  "version": "1.0.0",
  "description": "Multi-Agent AI Orchestration System for Social Media",
  "main": "src/orchestrator/agent-orchestrator.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run src/example-usage.ts",
    "build": "tsc",
    "start": "bun run src/orchestrator/agent-orchestrator.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --ci --coverage --maxWorkers=2",
    "test:bun": "bun test",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx --quiet",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run format:check && npm run lint:check && npm run type-check && npm run test:ci",
    "validate:env": "bun scripts/validate-env.ts",
    "orchestrate": "bun run src/orchestrator/agent-orchestrator.ts",
    "demo": "bun run src/example-usage.ts",
    "cli": "bun run src/cli/cli-interface.ts",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:codegen": "playwright codegen"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@lanonasis/memory-client": "^1.0.1",
    "@lanonasis/memory-sdk-standalone": "^1.0.0",
    "@supabase/mcp-server-supabase": "^0.5.9",
    "@supabase/supabase-js": "^2.83.0",
    "@types/bull": "^3.15.9",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^166.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^13.0.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "openai": "^6.9.1",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.2.3",
    "redis": "^5.9.0",
    "twitter-api-v2": "^1.28.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.56.1",
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.5",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.4",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
