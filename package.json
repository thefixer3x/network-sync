{
  "name": "social-media-orchestrator",
  "version": "1.0.0",
  "description": "Multi-Agent AI Orchestration System for Social Media",
  "main": "src/orchestrator/agent-orchestrator.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run src/example-usage.ts",
    "build": "tsc",
    "start": "bun run src/orchestrator/agent-orchestrator.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest --ci --coverage --maxWorkers=2",
    "test:bun": "bun test",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run format:check && npm run lint:check && npm run type-check && npm run test:ci",
    "validate:env": "bun scripts/validate-env.ts",
    "orchestrate": "bun run src/orchestrator/agent-orchestrator.ts",
    "demo": "bun run src/example-usage.ts",
    "cli": "bun run src/cli/cli-interface.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.9",
    "@supabase/supabase-js": "^2.76.1",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "commander": "^11.1.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "googleapis": "^144.0.0",
    "inquirer": "^9.3.8",
    "node-cron": "^3.0.3",
    "openai": "^4.104.0",
    "twitter-api-v2": "^1.27.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@types/bun": "latest",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.12",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
