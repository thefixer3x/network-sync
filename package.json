    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.12",
    "@types/node-cron": "^3.0.11",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
